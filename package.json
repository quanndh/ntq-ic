{
  "name": "user-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android &&./gradlew clean && cd .. && npx react-native run-android ",
    "android-release": "react-native run-android --variant=Release",
    "ios": "cd ios && xcodebuild clean && cd .. && npx react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "unlink": "npm unlink",
    "link": "npx react-native link",
    "uninstall": "npm uninstall --save",
    "type-check": "tsc --pretty --noEmit",
    "pod": "cd ios && pod install && cd ..",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "pod-clear": "cd ios && pod deintegrate && cd ..",
    "typegen": "graphql-codegen --config codegen/typegen.yml",
    "apk": "cd android &&./gradlew clean && ./gradlew assembleRelease && cd ..",
    "codegen": "graphql-codegen --config codegen/codegen.js && yarn schema:codegen",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --enableOriginalQuery --depthLimit 5 && prettier --write --loglevel silent __generated__/**/*.gql"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.2.0",
    "@hookform/error-message": "^0.0.5",
    "@hookform/resolvers": "0.1.1",
    "@react-native-community/async-storage": "2.0.0-rc.1",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.8.0",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "5.7.3",
    "@react-navigation/stack": "5.9.0",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.165",
    "@types/react-native-calendars": "^1.505.0",
    "@types/react-native-modalbox": "^1.4.8",
    "@types/react-native-snap-carousel": "^3.8.2",
    "@types/react-native-table-component": "^1.2.0",
    "@types/socket.io-client": "^1.4.34",
    "axios": "0.20.0",
    "engine.io-client": "^4.0.6",
    "extract-files": "9.0.0",
    "flat": "^5.0.2",
    "graphql": "15.3.0",
    "haversine-distance": "^1.2.1",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-hook-form": "6.8.2",
    "react-native": "0.63.3",
    "react-native-ble-plx": "^2.0.2",
    "react-native-device-info": "^8.1.3",
    "react-native-fast-image": "8.3.0",
    "react-native-geolocation-service": "^5.2.0",
    "react-native-gesture-handler": "1.8.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-hooks": "0.9.0",
    "react-native-image-picker": "^3.1.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-localize": "1.4.1",
    "react-native-modal": "^11.5.6",
    "react-native-modalbox": "^2.0.2",
    "react-native-reanimated": "1.13.0",
    "react-native-restart": "^0.0.20",
    "react-native-safe-area-context": "3.1.7",
    "react-native-screens": "2.10.1",
    "react-native-sensitive-info": "5.5.8",
    "react-native-simple-toast": "^1.1.3",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "7.1.0",
    "react-native-webview": "^11.2.1",
    "rn-fetch-blob": "^0.12.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/runtime": "7.11.2",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/near-operation-file-preset": "1.17.11",
    "@graphql-codegen/schema-ast": "1.17.8",
    "@graphql-codegen/typescript": "1.17.9",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/extract-files": "8.1.0",
    "@types/flat": "5.0.1",
    "@types/jest": "26.0.13",
    "@types/node": "14.10.1",
    "@types/react": "16.9.49",
    "@types/react-native": "0.63.18",
    "@types/react-test-renderer": "16.9.3",
    "@types/yup": "0.29.7",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-jest": "26.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.9.0",
    "eslint-plugin-graphql": "4.0.0",
    "gql-generator-type": "1.0.17",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "metro-react-native-babel-preset": "0.63.0",
    "prettier": "2.1.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "typescript": "4.0.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  }
}
